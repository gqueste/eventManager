<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>eventManager</display-name>
  <servlet>
    <description></description>
    <display-name>Index</display-name>
    <servlet-name>Index</servlet-name>
    <servlet-class>com.eventManager.servlets.Index</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Index</servlet-name>
    <url-pattern></url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Events</display-name>
    <servlet-name>Events</servlet-name>
    <servlet-class>com.eventManager.servlets.Events</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Events</servlet-name>
    <url-pattern>/events</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LogIn</display-name>
    <servlet-name>LogIn</servlet-name>
    <servlet-class>com.eventManager.servlets.LogIn</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogIn</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ConnexionHandler</display-name>
    <servlet-name>ConnexionHandler</servlet-name>
    <servlet-class>com.eventManager.servlets.ConnexionHandler</servlet-class>
  </servlet>
  <error-page>
  	<error-code>404</error-code>
    <location>/html/error_404.html</location>
</error-page>
</web-app>